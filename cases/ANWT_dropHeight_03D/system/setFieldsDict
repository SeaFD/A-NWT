FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
    volScalarFieldValue zoneID 123
);

regions
(
    boxToCell
    {
        box ( -1000 -1000 -1000 ) ( 1000 1000 0);
 	//box ( -100 -100 -100 ) ( 100 100 0);
        fieldValues ( volScalarFieldValue alpha.water 1 );
    }


);

